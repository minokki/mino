package com.lec.basic.poly3;

public class AppleSpeaker implements Speaker{

	
	public AppleSpeaker() {	
		System.out.println("==> appleSpeaker 객체 생성!");
	}
	
	public void volumnUp() {System.out.println("==> appleSpeaker 볼륨업");	}

	public void volumnDown() {	System.out.println("==> appleSpeaker 볼륨다운");}

}
